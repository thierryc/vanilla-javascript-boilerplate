{
  "name": "vanilla-javascript-boilerplate",
  "source": "src/index.html",
  "browserslist": "> 0.2%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel",
    "build": "rimraf dist && parcel build",
    "build:reporter": "rimraf dist && parcel build --reporter @parcel/reporter-bundle-analyzer",
